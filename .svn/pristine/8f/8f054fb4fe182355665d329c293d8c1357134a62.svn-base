package ma.dev.sowondejong.data;

public class Img {

	private String data;
	private String name = "";
		
	public Img(String name, String data) {
		this.setName(name);
		this.setData(data);
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getData() {
		return data;
	}

	public void setData(String data) {
		this.data = data;
	}
	
	public Img() {}
	
}
